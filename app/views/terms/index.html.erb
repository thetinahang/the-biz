<p id="notice"><%= notice %></p>

<h1>Terms</h1>

<table>
  <thead>
    <tr>
      <th>Method</th>
      <th>Artist</th>
      <th>Track</th>
      <th>Query Id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @terms.each do |term| %>
      <tr>
        <td><%= term.method %></td>
        <td><%= term.artist %></td>
        <td><%= term.track %></td>
        <td><%= term.query_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back to Query', query_path(@terms.first.query) %>

